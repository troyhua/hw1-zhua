<?xml version="1.0" encoding="UTF-8"?>
<typeSystemDescription xmlns="http://uima.apache.org/resourceSpecifier">
    <name>hw1-zhua-typesystem</name>
    <description>Type System Definition for Homework 1 of 11-791</description>
    <version>1.0</version>
    <vendor>Troy Hua(zhua)</vendor>
  <types>
    <typeDescription>
      <name>model.troy.hw.BaseType</name>
      <description/>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>source</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>confidence</name>
          <description/>
          <rangeTypeName>uima.cas.Double</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>model.troy.hw.Question</name>
      <description/>
      <supertypeName>model.troy.hw.BaseType</supertypeName>
      </typeDescription>
  <typeDescription>
      <name>model.troy.hw.Answer</name>
      <description/>
      <supertypeName>model.troy.hw.BaseType</supertypeName>
      <features>
        <featureDescription>
          <name>isCorrect</name>
          <description/>
          <rangeTypeName>uima.cas.Boolean</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>model.troy.hw.Token</name>
      <description/>
      <supertypeName>model.troy.hw.BaseType</supertypeName>
    </typeDescription>
    <typeDescription>
      <name>model.troy.hw.NGram</name>
      <description/>
      <supertypeName>model.troy.hw.BaseType</supertypeName>
      <features>
        <featureDescription>
          <name>elements</name>
          <description/>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>model.troy.hw.Token</elementType>
          <multipleReferencesAllowed>true</multipleReferencesAllowed>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>model.troy.hw.AnswerScore</name>
      <description/>
      <supertypeName>model.troy.hw.BaseType</supertypeName>
      <features>
        <featureDescription>
          <name>answer</name>
          <description/>
          <rangeTypeName>model.troy.hw.Answer</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>model.troy.hw.Evaluation</name>
      <description/>
      <supertypeName>model.troy.hw.BaseType</supertypeName>
      <features>
        <featureDescription>
          <name>N</name>
          <description>N is the one in precision at N, which is also the total number of correct answers.</description>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>precision</name>
          <description>The precision at N.</description>
          <rangeTypeName>uima.cas.Double</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
  </types>
</typeSystemDescription>
